

{% for banner in banners %}
  <div class="swiper-slide">
    {% if banner.link %}<a href="{{ banner.link }}"><img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive" /></a>{% else %}<img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive" style="width: 100%"/>{% endif %}
   
  </div>
  {% endfor %}
